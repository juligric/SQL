EJ6
SELECT count (*) 
from alumno
where nombre like '%Gomez';

EJ7
SELECT count (*) 
from alumno
where nombre like 'L%';

EJ8
SELECT * FROM alumno
WHERE EXTRACT(MONTH FROM fecha_nac) = 4
  AND EXTRACT(DAY FROM fecha_nac) = 19;






