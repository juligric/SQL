EJ 1
SELECT * FROM alumno;
where fecha_nac > '1994 - 12 - 31' 
order by nombres ASC

EJ2
SELECT * FROM alumno
where  email like '%@hotmail.com';

EJ3

update especialidad
set codigo = 'NT' 
where  codigo = 'NR';


